<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.webapp.project.Login</servlet-class>

	</servlet>
	<servlet-mapping>

		<servlet-name>Login</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Logout</servlet-name>
		<servlet-class>com.webapp.project.Logout</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/Logout</url-pattern>
	</servlet-mapping>
	<servlet>
	<servlet-name>Profile</servlet-name>
	<servlet-class>com.webapp.project.Profile</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>Profile</servlet-name>
	<url-pattern>/Profile</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
	<welcome-file>link.html</welcome-file>
	<welcome-file>index.html</welcome-file>	
	<welcome-file>details.html</welcome-file>	
	</welcome-file-list>
</web-app>
